{
	"name": "magpix",
	"publisher": "crawron",
	"license": "MIT",
	"displayName": "Magpix",
	"description": "Pixel Perfect Icons for Visual Studio Code",
	"icon": "images/market-icon.png",
	"galleryBanner": {
		"color": "#1F2428",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Crawron/Magpix.git"
	},
	"version": "0.0.5",
	"engines": {
		"vscode": "*"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "magpix",
				"label": "Magpix",
				"path": "./dist/build-icon-theme.json"
			}
		]
	},
	"scripts": {
		"compile": "ts-node -P ./tsconfig.json src/compile.ts",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^14.14.31",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.2",
		"vsce": "^1.85.0"
	}
}
